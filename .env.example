# FluxAI Gateway Configuration

# Application
APP_NAME=FluxAI Gateway
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Server
HOST=0.0.0.0
PORT=8080
WORKERS=4
RELOAD=true

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_BEDROCK_ENDPOINT=https://bedrock-runtime.us-east-1.amazonaws.com

# Database
DATABASE_URL=postgresql+asyncpg://fluxai:dev_password@localhost:5432/fluxai
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=10

# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_MAX_CONNECTIONS=50

# Security
SECRET_KEY=your-secret-key-change-in-production
API_KEY_HASH_ALGORITHM=HS256
RATE_LIMIT_ENABLED=true

# Observability
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090
OPENTELEMETRY_ENABLED=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600
CACHE_SIMILARITY_THRESHOLD=0.95
CACHE_SEMANTIC_ENABLED=true
CACHE_EXACT_MATCH_FIRST=true

# Embedding Configuration
EMBEDDING_MODEL=amazon.titan-embed-text-v1
EMBEDDING_DIMENSION=1536
EMBEDDING_BATCH_SIZE=25
EMBEDDING_CACHE_TTL=86400

# Cost Tracking
ENABLE_COST_TRACKING=true
PRICING_UPDATE_INTERVAL_HOURS=24
